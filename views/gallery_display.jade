extends layout

block content
  - each photo_id in photo_ids
    div
      p
        a(href="/viewphoto/#{photo_id}")
          img(src="/thumbnail/#{photo_id}")

block sidebar
  .sidebar_widget
    h1 Menu
    p
      ul
        li
          a(href='/home') Home

        li
          a(href='/photo/new') New Photo

        li
          a(href='/photo') Browse Photos

        li
          a(href='/logout') Logout
